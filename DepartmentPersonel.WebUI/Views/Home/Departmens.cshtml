@model List<DepartmentPersonel.Entities.Department>


<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Departman Id</th>
            <th>Departman Adı</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>

